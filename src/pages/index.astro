---
import Header from '../components/Header.astro';
import Layout from '../layouts/Layout.astro';
import Sectior from '../components/Sectior.astro';
import ModelY from  '../components/ModelY.astro';
import ModelX from '../components/ModelX.astro';
import ModelS from '../components/ModelS.astro';
import Model3 from '../components/Model3.astro';
import Accessories from '../components/Accessories.astro';
import PoweWall from '../components/PowerWall.astro';
---

<Layout title="Tesla-Welcome to the future">
	<Header />
	<main class="snap-y snap-mandatory relative w-full h-screen overflow-auto">
		
		

		<div class="snap-center">
		<Sectior />
	</div>
	<div class="snap-center">
		<ModelS />
	</div>
	<div class="snap-center">
		<Model3 />
	</div>
	<div class="snap-center">
		<ModelX />
	</div>
	<div class="snap-center">
		<ModelY />
	</div>
	
	
	
	<div class="snap-center">
		<PoweWall />
		
	</div>
	<div class="snap-center">
		<Accessories />
		
	</div>
	
	
	
		
	</main>
</Layout>  